{
  "name": "chakra-combobox",
  "version": "4.0.0",
  "description": "A combobox componente for Chakra UI.",
  "author": "daniel-dnb",
  "license": "MIT",
  "type": "module",
  "homepage": "https://daniel-dnb.github.io/chakra-combobox",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daniel-dnb/chakra-combobox"
  },
  "keywords": [
    "react",
    "async",
    "combobox",
    "autocomplete",
    "dropdown",
    "virtualized",
    "chakra-ui",
    "radix-ui",
    "tanstack",
    "ui",
    "component",
    "search",
    "react-components",
    "library"
  ],
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepublishOnly": "pnpm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build --output-dir docs"
  },
  "dependencies": {
    "@tanstack/react-virtual": "^3.13.12"
  },
  "peerDependencies": {
    "@chakra-ui/react": "^3.26.0",
    "react": "18.x || 19.x"
  },
  "devDependencies": {
    "@chakra-ui/react": "^3.26.0",
    "@chromatic-com/storybook": "^3.2.4",
    "@emotion/react": "^11.14.0",
    "@eslint/js": "^9.34.0",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-onboarding": "^8.6.0",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^8.6.0",
    "@storybook/react-vite": "^8.6.0",
    "@storybook/test": "^8.6.14",
    "@tanstack/react-query": "^5.85.5",
    "@types/node": "^24.3.0",
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "eslint": "^9.34.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^0.11.3",
    "globals": "^16.3.0",
    "globby": "^14.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "storybook": "^8.6.0",
    "typescript": "5.9.2",
    "typescript-eslint": "^8.41.0",
    "vite": "^7.1.3",
    "vite-plugin-dts": "^4.5.4"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
