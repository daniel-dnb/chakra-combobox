{
  "name": "chakra-combobox",
  "version": "2.0.2",
  "description": "A combobox componente for Chakra UI.",
  "author": "daniel-dnb",
  "license": "MIT",
  "type": "module",
  "homepage": "https://daniel-dnb.github.io/chakra-combobox",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daniel-dnb/chakra-combobox"
  },
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepublishOnly": "pnpm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build --output-dir docs"
  },
  "dependencies": {
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@tanstack/react-virtual": "^3.13.2"
  },
  "peerDependencies": {
    "@chakra-ui/react": "2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "framer-motion": "^12.4.7",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.4",
    "@eslint/js": "^9.21.0",
    "@storybook/addon-essentials": "^8.6.0",
    "@storybook/addon-interactions": "^8.6.0",
    "@storybook/addon-onboarding": "^8.6.0",
    "@storybook/blocks": "^8.6.0",
    "@storybook/react": "^8.6.0",
    "@storybook/react-vite": "^8.6.0",
    "@storybook/test": "^8.6.0",
    "@tanstack/react-query": "^5.66.9",
    "@types/node": "^22.13.5",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-storybook": "^0.11.3",
    "globals": "^15.15.0",
    "globby": "^14.1.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "storybook": "^8.6.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.5.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
